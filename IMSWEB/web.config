    </modules>
    <handlers>
      <remove name="ExtensionlessUrlHandler-Integrated-4.0"/>
      <remove name="OPTIONSVerbHandler"/>
      <remove name="TRACEVerbHandler"/>
      <add name="ExtensionlessUrlHandler-Integrated-4.0" path="*." verb="*" type="System.Web.Handlers.TransferRequestHandler" preCondition="integratedMode,runtimeVersionv4.0"/>
      <add name="ReportViewerWebControlHandler" verb="*" path="Reserved.ReportViewerWebControl.axd" preCondition="integratedMode" type="Microsoft.Reporting.WebForms.HttpHandler, Microsoft.ReportViewer.WebForms, Version=14.0.0.0, Culture=neutral, PublicKeyToken=89845DCD8080CC91"/>
    </handlers>
    <security>
      <requestFiltering>
        <requestLimits maxQueryString="32768"/>
      </requestFiltering>
    </security>
  </system.webServer>
  <log4net>
    <appender name="INFOLogFileAppender" type="log4net.Appender.RollingFileAppender">
      <file value="..\OCTLogs\ARIAN\"/>
      <datePattern value="'ARIANLog'-dd.MM.yyyy'.txt'"/>
      <appendToFile value="true"/>
      <rollingStyle value="Date"/>
      <maxSizeRollBackups value="5"/>
      <maximumFileSize value="10MB"/>
      <staticLogFileName value="false"/>
      <lockingModel type="log4net.Appender.FileAppender+MinimalLock"/>
      <layout type="log4net.Layout.SerializedLayout, log4net.Ext.Json">
        <decorator type="log4net.Layout.Decorators.StandardTypesDecorator, log4net.Ext.Json"/>
        <default/>
        <remove value="appname"/>
        <remove value="ndc"/>
        <remove value="message"/>
        <remove value="thread"/>
        <member value="BrandId:BrandId"/>
        <member value="method:method"/>
        <member value="message:messageobject"/>
      </layout>
      <filter type="log4net.Filter.LevelRangeFilter">
        <levelMin value="ALL"/>
      </filter>
    </appender>
    <root>
      <appender-ref ref="INFOLogFileAppender"/>
      <appender-ref ref="ERRORLogFileAppender"/>
    </root>
  </log4net>
