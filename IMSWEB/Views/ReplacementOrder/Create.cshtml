    $(document).on('change', 'input.chkbxProduct', function () {
        $('input.chkbxProduct').not(this).prop('checked', false);
        var values = this.value.split(','); 
        var ProductId = values[0]; 
        var IMENo = values[1];
        SelectProductDetails(IMENo, ProductId);
    });

    //for damage Product
    function SelectProductDetails(IMENo, ProductId) {  
        if(IMENo==""){
            ClearDamageProductDetails();
            alert('firedoo');
            return;
